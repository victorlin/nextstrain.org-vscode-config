{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.js",
            "env": {
                "NODE_ENV": "dev",
                "GATSBY_DEV_URL": "http://localhost:8000"
            },
            "preLaunchTask": "start-gatsby-server",
            "postDebugTask": "stop-gatsby-server"
        }
    ]
}